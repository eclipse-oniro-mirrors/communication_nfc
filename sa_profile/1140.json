{
    "process": "nfc_service",
    "systemability": [
        {
            "name": 1140,
            "libpath": "libnfc_service.z.so",
            "run-on-create": false,
            "auto-restart": true,
            "distributed": false,
            "dump_level": 1,
            "min_hdi_proxy_version": [],
            "start-on-demand":{
                "commonevent":[
                    {
                        "name":"usual.event.USER_UNLOCKED",
                        "conditions":[
                            {
                                "eventId":"param",
                                "name":"persist.nfc.switch.state",
                                "value":"3"
                            }
                        ]
                    },
                    {
                        "name":"usual.event.BOOT_COMPLETED",
                        "conditions":[
                            {
                                "eventId":"param",
                                "name":"persist.nfc.switch.state",
                                "value":"3"
                            }
                        ]
                    }
                ]
            }
        }
    ]
}